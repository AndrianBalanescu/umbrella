{
    "name": "@thi.ng/geom",
    "version": "2.1.29",
    "description": "Functional, polymorphic API for 2D geometry types & SVG generation",
    "type": "module",
    "module": "./index.js",
    "typings": "./index.d.ts",
    "sideEffects": false,
    "repository": {
        "type": "git",
        "url": "https://github.com/thi-ng/umbrella.git"
    },
    "homepage": "https://github.com/thi-ng/umbrella/tree/develop/packages/geom#readme",
    "funding": [
        {
            "type": "github",

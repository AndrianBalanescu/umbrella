{
    "name": "@thi.ng/ksuid",
    "version": "2.1.14",
    "description": "Configurable K-sortable unique IDs, ULIDs, binary & base-N encoded, 32/48/64bit time resolutions",
    "type": "module",
    "module": "./index.js",
    "typings": "./index.d.ts",
    "sideEffects": false,
    "repository": {
        "type": "git",
        "url": "https://github.com/thi-ng/umbrella.git"
    },
    "homepage": "https://github.com/thi-ng/umbrella/tree/master/packages/ksuid#readme",
    "funding": [
        {
            "type": "github",
            "url": "https://github.com/sponsors/postspectacular"
        },
        {
            "type": "patreon",
            "url": "https://patreon.com/thing_umbrella"
        }
    ],
    "author": "Karsten Schmidt <k+npm@thi.ng>",
    "license": "Apache-2.0",
    "scripts": {
        "bench": "tools:node-esm bench/index.ts",
        "build": "yarn clean && tsc --declaration",
        "clean": "rimraf '*.js' '*.d.ts' '*.map' doc",
        "doc": "typedoc --excludePrivate --excludeInternal --out doc src/index.ts",
        "doc:ae": "mkdir -p .ae/doc .ae/temp && api-extractor run --local --verbose",
        "doc:readme": "yarn doc:stats && tools:readme",
        "doc:stats": "tools:module-stats",
        "pub": "yarn npm publish --access public",
        "test": "testament test"
    },
    "dependencies": {
        "@thi.ng/base-n": "^2.3.4",
        "@thi.ng/errors": "^2.1.9",
        "@thi.ng/random": "^3.3.4",
        "@thi.ng/strings": "^3.3.8"
    },
    "devDependencies": {
        "@microsoft/api-extractor": "^7.25.0",
        "@thi.ng/testament": "^0.2.10",
        "rimraf": "^3.0.2",
        "tools": "workspace:^",
        "typedoc": "^0.22.17",
        "typescript": "^4.7.4"
    },
    "keywords": [
        "32bit",
        "64bit",
        "base62",
        "bigint",
        "binary",
        "bytes",
        "crypto",
        "database",
        "dynamodb",
        "encode",
        "epoch",
        "generator",
        "id",
        "millisecond",
        "random",
        "sort",
        "timestamp",
        "typescript",
        "ulid",
        "uuid"
    ],
    "publishConfig": {
        "access": "public"
    },
    "engines": {
        "node": ">=12.7"
    },
    "files": [
        "*.js",
        "*.d.ts"
    ],
    "exports": {
        ".": {
            "default": "./index.js"
        },
        "./aksuid": {
            "default": "./aksuid.js"
        },
        "./api": {
            "default": "./api.js"
        },
        "./ksuid32": {
            "default": "./ksuid32.js"
        },
        "./ksuid64": {
            "default": "./ksuid64.js"
        },
        "./ulid": {
            "default": "./ulid.js"
        }
    },
    "thi.ng": {
        "related": [
            "base-n",
            "idgen",
            "random"
        ],
        "status": "stable",
        "year": 2020
    }
}

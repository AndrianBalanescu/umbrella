{
    "name": "@thi.ng/rstream-gestures",
    "version": "4.0.5",
    "description": "Unified mouse, mouse wheel & multi-touch event stream abstraction",
    "type": "module",
    "module": "./index.js",
    "typings": "./index.d.ts",
    "sideEffects": false,
    "repository": {
        "type": "git",
        "url": "https://github.com/thi-ng/umbrella.git"
    },
    "homepage": "https://github.com/thi-ng/umbrella/tree/develop/packages/rstream-gestures#readme",
    "funding": [
        {
            "type": "github",
            "url": "https://github.com/sponsors/postspectacular"
        },
        {
            "type": "patreon",
            "url": "https://patreon.com/thing_umbrella"
        }
    ],
    "author": "Karsten Schmidt <k+npm@thi.ng>",
    "license": "Apache-2.0",
    "scripts": {
        "build": "yarn clean && tsc --declaration",
        "clean": "rimraf *.js *.d.ts *.map doc",
        "doc": "typedoc --excludePrivate --excludeInternal --out doc src/index.ts",
        "doc:ae": "mkdir -p .ae/doc .ae/temp && node_modules/.bin/api-extractor run --local --verbose",
        "doc:readme": "yarn doc:stats && ../../scripts/node-esm ../../tools/src/readme.ts",
        "doc:stats": "../../scripts/node-esm ../../tools/src/module-stats.ts",
        "pub": "yarn build && yarn publish --access public",
        "test": "testament test"
    },
    "dependencies": {
        "@thi.ng/api": "^8.0.4",
        "@thi.ng/checks": "^3.0.4",
        "@thi.ng/math": "^5.0.4",
        "@thi.ng/rstream": "^7.0.5",
        "@thi.ng/transducers": "^8.0.4"
    },
    "devDependencies": {
        "@thi.ng/testament": "^0.1.4"
    },
    "keywords": [
        "animation",
        "dataflow",
        "event",
        "interaction",
        "mouse",
        "mousewheel",
        "multitouch",
        "reactive",
        "rstream",
        "stream",
        "touch",
        "typescript"
    ],
    "publishConfig": {
        "access": "public"
    },
    "engines": {
        "node": ">=12.7"
    },
    "files": [
        "*.js",
        "*.d.ts"
    ],
    "exports": {
        ".": {
            "import": "./index.js"
        },
        "./api": {
            "import": "./api.js"
        },
        "./gesture-stream": {
            "import": "./gesture-stream.js"
        }
    },
    "thi.ng": {
        "parent": "@thi.ng/rstream",
        "related": [
            "hdom",
            "rdom"
        ],
        "year": 2018
    }
}
